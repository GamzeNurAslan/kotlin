fun main() {
var number1: Int = 89
    var number2: Int = 17
       number1+=number2
    println(number1)
       number1-=number2
    println(number1)
       number1*=number2
    println(number1)
       number1/=number2
    println(number1)
       number1%=number2
    println(number1)
       ++number1
    println(number1)
       number1++
    println(number1)
       --number2
    println(number2)
       number2--
    println(number2)  
}
